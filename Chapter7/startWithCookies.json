[
  {
    "description": "这是一个带cookies信息的get请求",
    "request": {
      "uri": "/get/with/cookies",
      "method": "get",
      "cookies": {
        "login": "true"
      }
    },
    "response": {
      "text": "这是一个需要携带cookies信息才能访问的get请求"
    }
  },

  {
    "description": "这是一个带cookies信息的post请求",
    "request": {
      "uri": "/post/with/cookies",
      "method": "post",
      "cookies": {
        "login": "true"
      },
      "json": {
        "name": "huhansan",
        "age": "18"
      }
    },
    "response": {
      "status": 200,
      "json": {
        "huhansan": "success",
        "status": "1"
      }
    }
  },
  {
    "description": "这是一个带headers信息的post请求",
    "request": {
      "uri": "/post/with/headers",
      "method": "post",
      "headers": {
        "content-type": "application/json"
      },
      "json": {
        "name": "zhaoliu",
        "age": "29"
      }
    },
    "response": {
      "status": 200,
      "json": {
        "zhaoliu": "success",
        "status": "1"
      }
    }
  }

]